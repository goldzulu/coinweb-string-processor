  "dependencies": {
    "@ant-design/icons": "^5.3.7",
    "@apollo/explorer": "^3.6.0",
    "@coinweb/wallet-lib": "^0.1.113-esm",
    "antd": "^5.17.4",
    "highlight.js": "^11.9.0",
    "p-debounce": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "rehype-highlight": "^7.0.0",
    "string-processor.cm": "workspaces:*",
    "use-deep-compare-effect": "^1.8.1"
  },
  "devDependencies": {
    "@mdx-js/rollup": "^3.0.1",
    "@testing-library/jest-dom": "^6.4.5",
